{
"manifest_version":3,
"name":"Browser Buddies",
"description":"Chrome Extension easing sharing of bookmarks and tabs",
"permissions": ["tabs","activeTab"],

"version":"1.1",
"browser_action":{
"default_popup":"index.html"
},
"background": {
    "page": "index.html",
    "persistent": false
  },
"content_security_policy":"script-src 'self' 'unsafe-eval'; object-src 'self'",
"web_accessible_resources":[
"assets/css/*",
"assets/js/*",
"assets/fonts/*"
]
}
